# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# ----------------- EMBEDDING CONFIG -----------------
NEXT_PUBLIC_EMBEDDING_MODEL_NAME="gte-small"
NEXT_PUBLIC_EMBEDDING_POOLING_METHOD="mean" # mean pooling is the default method for gte-small
NEXT_PUBLIC_EMBEDDING_NORMALIZED=true # normalized embeddings are more performant in the dot product
